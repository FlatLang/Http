package flat/http

import flat/datastruct/HashMap

class {
  public static async get(
    String url,
    HashMap<String, String> headers = HashMap()
  ) => Request(
      url,
      method: "GET",
      headers: headers
    ).run()

  public static class Global {
    external type HttpsType
    visible static lazy HttpsType https {
      get {
        external {
          return require('https');
        }
      }
    }
  }
}
